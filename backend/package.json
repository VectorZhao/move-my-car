    "prisma": "5.10.2",
    "zod": "3.23.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.6",
    "@types/cookie-parser": "1.4.7",
    "@types/express": "4.17.21",
    "@types/cors": "2.8.17",
    "@types/jsonwebtoken": "9.0.6",
    "@types/morgan": "1.9.7",
    "@types/node": "20.12.7",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
